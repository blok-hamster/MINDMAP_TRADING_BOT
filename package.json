{
  "name": "kol-mindmap-trading-bot",
  "version": "1.0.0",
  "description": "Autonomous trading bot that leverages KOL mindmap data for Solana memecoin trading",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "type-check": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "echo \"No linter configured\"",
    "prod": "npm run build && npm start",
    "reset-paper": "ts-node src/scripts/reset_paper_trading.ts"
  },
  "keywords": [
    "trading",
    "bot",
    "solana",
    "kol",
    "mindmap",
    "memecoin"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.5",
    "bn.js": "^5.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.2.1",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "redis": "^4.6.12",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "@types/bn.js": "^5.2.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^20.10.6",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}