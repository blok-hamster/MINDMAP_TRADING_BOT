{
  "name": "kol-mindmap-trading-bot",
  "version": "1.0.0",
  "description": "Autonomous trading bot that leverages KOL mindmap data for Solana memecoin trading",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "type-check": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "echo \"No linter configured\"",
    "prod": "npm run build && npm start"
  },
  "keywords": [
    "trading",
    "bot",
    "solana",
    "kol",
    "mindmap",
    "memecoin"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.5",
    "dotenv": "^16.6.1",
    "redis": "^4.6.12",
    "socket.io-client": "^4.7.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
